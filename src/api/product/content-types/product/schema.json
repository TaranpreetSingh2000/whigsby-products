{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "products",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "float",
      "required": true,
      "unique": true
    },
    "rating": {
      "type": "decimal"
    },
    "category": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "discount": {
      "type": "biginteger",
      "required": true,
      "default": "40"
    },
    "offer": {
      "type": "string"
    },
    "mrp": {
      "type": "float"
    }
  }
}
